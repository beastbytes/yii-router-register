{
  "name": "beastbytes/yii-router-register",
  "description": "RouterRegister Writer and console command for Yii",
  "keywords": [
    "attributes",
    "router",
    "routes",
    "yii3"
  ],
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "chris",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "beastbytes/router-register": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/console": "^6.0",
    "vimeo/psalm": "^6.0",
    "yiisoft/dummy-provider": "^1.0",
    "yiisoft/files": "^2.0",
    "yiisoft/router": "^4.0",
    "yiisoft/yii-console": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Yii\\Router\\Register\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Yii\\Router\\Register\\Tests\\": "tests"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "di-console": "di-console.php",
      "params-console": "params-console.php"
    }
  }
}